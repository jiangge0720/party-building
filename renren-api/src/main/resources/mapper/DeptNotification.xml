<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.renren.dao.DeptNotificationDao">
    <select id="selectDeptId" parameterType="Long" resultType="io.renren.entity.DeptNotification">
        select * from tb_dept_notification where dept_id = #{deptId}
